{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js",
                    "/fa-**"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "src/assets/**",
                    "https://assets.beerless.be/**"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "api",
            "urls": ["https://api.beerless.be/**"],
            "cacheConfig": {
                "Strategy": "freshness",
                "maxSize": 100,
                "maxAge": "2h",
                "timeout": "2s"
            }
        }
    ]
}
